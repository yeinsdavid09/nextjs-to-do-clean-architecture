enum NotificationType {
  INFO
  WARNING
  ERROR
  SUCCESS
}

model Notification {
  id               String             @id @default(uuid())
  key              String             @db.VarChar(50)
  content          Json
  type             NotificationType   @default(INFO)
  userNotification UserNotification[]
  createdAt        DateTime           @default(now())
  updatedAt        DateTime           @updatedAt
}
